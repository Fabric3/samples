    <runtime port.range="1090-1190"/>
    <web.server>
        <http port="8181"/>
    </web.server>
    <scanner production="true"/>
    <templates>
        <template name="ProviderChannelBinding">
            <binding.zeromq addresses="127.0.0.1:12001"/>
        </template>
        <template name="ReplicationChannelBinding">
            <binding.zeromq addresses="127.0.0.1:13001"/>
        </template>
    </templates>

</config>
